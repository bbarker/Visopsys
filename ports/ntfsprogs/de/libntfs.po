#: ntfsprogs/ntfsresize.c:878
#, c-format
msgid "Relocation needed for inode %8lld"
msgstr "Es wird eine Neuplatzierung benötigt für inode %8lld"

#: ntfsprogs/ntfsresize.c:1036
msgid "Accounting clusters"
msgstr "Die Cluster werden berechnet"

#: ntfsprogs/ntfsresize.c:1186
msgid "Checking filesystem consistency"
msgstr "Die Konsistenz des Dateisystems wird überprüft"

#: ntfsprogs/ntfsresize.c:1270
msgid "Collecting resizing constraints"
msgstr "Die Bedingungen für die Größenanpassung werden zusammengestellt"

#: ntfsprogs/ntfsresize.c:1375
msgid "Enlarging attribute header ..."
msgstr "Die Attribut-Kopfzeile wird vergrößert..."

#: ntfsprogs/ntfsresize.c:1797
#, c-format
msgid "Relocate record %llu 0x%llx->0x%llx"
msgstr "Eintrag %llu 0x%llx->0x%llx wird neu platziert"

#: ntfsprogs/ntfsresize.c:1985
msgid "Relocating needed data"
msgstr "Die benötigten Daten werden neu platziert"

#: ntfsprogs/ntfsresize.c:2356
msgid "Checking for bad sectors"
msgstr "Überprüfung auf falsche Sektoren"

#: ntfsprogs/ntfsresize.c:2424
msgid "Updating $BadClust file"
msgstr "Aktualisierung der Datei $BadClust"

#: ntfsprogs/ntfsresize.c:2450
msgid "Updating $Bitmap file"
msgstr "Aktualisierung der Datei $Bitmap"

#: ntfsprogs/ntfsresize.c:2478
msgid "Set up LCN bitmap"
msgstr "LCN-Bitmap wird vorbereitet"

#: ntfsprogs/ntfsresize.c:2501
msgid "Updating Boot record"
msgstr "Boot-Eintrag wird aktualisiert"

#: ntfsprogs/ntfsresize.c:2601
msgid "Mounting volume"
msgstr "Laufwerk wird verbunden"

#: ntfsprogs/ntfsresize.c:2684
msgid "Schedule chkdsk for NTFS consistency check at Windows boot time"
msgstr ""
"Zeitliche Durchführung von chkdsk für die Überprüfung der NTFS-Konsistenz "
"beim Start von Windows festlegen"

#: ntfsprogs/ntfsresize.c:2699
msgid "Resetting $LogFile (this might take a while)"
msgstr "Zurücksetzen von $LogFile (das könnte einen Moment dauern)"

#: ntfsprogs/ntfsresize.c:3006
msgid "Syncing device"
msgstr "Gerät wird synchronisiert"

#: ntfsprogs/ntfsresize.c:3013
#, c-format
msgid "Successfully resized NTFS on device '%s'."
msgstr "NTFS wurde erfolgreich auf dem Gerät '%s' in der Größe angepasst"
